<div id="<%= dom_id user %>" class="scaffold_record">
  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Token:</strong>
    <%= user.token %>
  </p>

  <p>
    <strong>Token expires at:</strong>
    <%= user.token_expires_at %>
  </p>

  <p>
    <%= link_to "Show this user", user %>
  </p>
</div>
